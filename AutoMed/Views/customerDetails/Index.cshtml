
@model AutoMed.Models.Customer



<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://code.jquery.com/mobile/1.4.5/jquery.mobile-1.4.5.min.css">
    <script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src="https://code.jquery.com/mobile/1.4.5/jquery.mobile-1.4.5.min.js"></script>

</head>
<h2>Index</h2>
<body>
           
            <h1>Customer Details</h1>
        
         @Html.ActionLink("Back to Customers", "Index", "Customers", new {  }, new { })
           
        
    
  <h1> @Html.DisplayFor(modelItem => Model.FirstName)  @Html.DisplayFor(modelItem => Model.LastName)</h1> 
   
      
    <div data-role="main" class="ui-content">
            <div data-role="collapsible">
                <h1>Vehicles</h1>
                <p>
                <p>
                    @Html.ActionLink("Create New", "Create_V")
                </p>
               
                 <table class="table table-striped">
                    <tr>
                        <th>
                            "vin"
                        </th>
                        <th>
                            "make"
                        </th>
                        <th>
                            "model"
                        </th>
                        <th>
                            color
                        </th>
                        <th>
                            year
                        </th>
                        <th>
                            License Plate
                        </th>
                        <th></th>
                    </tr>

                    

                    @for (int i = 0; i < Model.Vehicles.Count; i++)
    {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => Model.Vehicles[i].Vin)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.Vehicles[i].Make)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.Vehicles[i].Model)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.Vehicles[i].Color)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.Vehicles[i].Year)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.Vehicles[i].LicensePlate)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit_V", new { id = Model.Vehicles[i].Id }) |
                        @Html.ActionLink("Details", "Details_V", new { id = Model.Vehicles[i].Id }) |
                        @Html.ActionLink("Delete", "Delete_V", new { id = Model.Vehicles[i].Id })
                    </td>
                </tr>
            }
                
                </table>

            </div>
        </div>

        <div data-role="main" class="ui-content">
            <div data-role="collapsible">
                <h1>Quotes</h1>
                <p>
                    @Html.ActionLink("Create New", "Create_Q")
                </p>
                <table class="table table-striped">
                    <tr>
                       @* <th>
        Persons in Houshold
    </th>*@
                        <th>
                            Date Created
                        </th>
                        <th>
                           Date Review
                        </th>
                        <th>
                            Discount %
                        </th>
                        <th>
                           Total Cost
                        </th>
                        <th>
                           Approval
                        </th>
                       @* <th>
       Description
    </th>*@
                        <th></th>
                    </tr>
                    
                   @for (int j = 0; j < Model.Quotes.Count; j++)
    {
        <tr>
           @* <td>
        @Html.DisplayFor(modelItem => Model.Quotes[j].CurrentNumberInHousehold)
    </td>*@
            <td>
                @Html.DisplayFor(modelItem => Model.Quotes[j].DateCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Quotes[j].DateReview)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Quotes[j].DiscountPercentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Quotes[j].TotalCost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.Quotes[j].Approved)
            </td>
            @*<td>
        @Html.DisplayFor(modelItem => Model.Quotes[j].WorkDescription)
    </td>*@
            <td>
                @Html.ActionLink("Edit", "Edit_Q", new { id = Model.Quotes[j].Id }) |
                @Html.ActionLink("Details", "Details_Q", new { id = Model.Quotes[j].Id }) |
                @Html.ActionLink("Delete", "Delete_Q", new { id = Model.Quotes[j].Id })
            </td>
        </tr>
    }
                    
                </table>
                
            </div>
        </div>
    
    
</body>
