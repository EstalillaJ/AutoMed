@model IEnumerable<AutoMed.Models.CustomerDetails>

@{
    ViewBag.Title = "Index";
}
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>jQuery UI Accordion - Default functionality</title>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script>
  $( function() {
    $( "#accordion" ).accordion();
  } );
    </script>
</head>
<h2>Index</h2>
<div id="accordion">
    <div>
        <h3>Vehicles</h3>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Vehicle.Vin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Vehicle.Make)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Vehicle.Model)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Vehicle.Color)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Vehicle.Year)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Vehicle.LicensePlate)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vehicle.Vin)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vehicle.Make)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vehicle.Model)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vehicle.Color)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vehicle.Year)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vehicle.LicensePlate)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Vehicle.Id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.Vehicle.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Vehicle.Id })
                    </td>
                </tr>
            }

        </table>

    </div>
    <div>


        <h2>Index</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.CurrentNumberInHousehold)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.DateCreated)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.DateReview)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.DiscountPercentage)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.TotalCost)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.Approved)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quote.WorkDescription)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.CurrentNumberInHousehold)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.DateCreated)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.DateReview)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.DiscountPercentage)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.TotalCost)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.Approved)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quote.WorkDescription)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Quote.Id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.Quote.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Quote.Id })
                    </td>
                </tr>
            }

        </table>


    </div>
</div>
   
